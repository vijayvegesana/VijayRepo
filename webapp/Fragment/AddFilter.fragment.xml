						emptySpanM="0" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false"/>
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
						
								<f:FormElement>
								<f:label>
									<Label text="{i18n>area}" design="Bold"></Label>
								</f:label>
								<f:fields>
									<ComboBox  class="input" width="80%"
										selectionChange="onRailOperationChange"	>													
										<ListItem xmlns="sap.ui.core" />
									</ComboBox>
								</f:fields>
							</f:FormElement>
							<f:FormElement>
								<f:label>
									<Label design="Bold" text="{i18n>functionLocation}"></Label>
								</f:label>
								<f:fields>
									<ComboBox  class="input" width="80%"
										selectionChange="onRailOperationChange"	>													
										<ListItem xmlns="sap.ui.core"  />
									</ComboBox>
								</f:fields>
							</f:FormElement>
							<f:FormElement>
								<f:label>
									<Label text="{i18n>basicStartDate}" design="Bold"></Label>
								</f:label>
								<f:fields>
									<DateTimePicker  enabled="true" value = "" valueFormat="yyyyMMdd:HHmmss" id="idRailOperationEDT2" 
										 change="onEndDateChange" placeholder="Enter Date &amp; Time ..." width="80%"
										class="input" />
								</f:fields>
							</f:FormElement>
				
						
						
							<f:FormElement>
								<f:label>
									<Label text="{i18n>productionZone}" design="Bold"></Label>
								</f:label>
								<f:fields>
									<ComboBox  class="input"  width="80%"
										selectionChange="onRailOperationChange"	>													
										<ListItem xmlns="sap.ui.core" />
									</ComboBox>
									
								</f:fields>
							</f:FormElement>
						
							<f:FormElement>
								<f:label>
									<Label text="{i18n>fiscalYear}" design="Bold"></Label>
								</f:label>
								<f:fields>
									<ComboBox  class="input" width="80%"
										selectionChange="onRailOperationChange"	>													
										<ListItem xmlns="sap.ui.core" />
									</ComboBox>
								</f:fields>
							</f:FormElement>
							<f:FormElement>
								<f:label>
									<Label design="Bold" text="{i18n>season}"></Label>
								</f:label>
								<f:fields>
									<ComboBox  class="input" width="80%"
										selectionChange="onRailOperationChange"	>													
										<ListItem xmlns="sap.ui.core" />
									</ComboBox>
								</f:fields>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
						
					
				</f:formContainers>
			</f:Form>	
			
			
